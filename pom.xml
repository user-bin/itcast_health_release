<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.itheima</groupId>
    <artifactId>itcast_health</artifactId>
    <packaging>pom</packaging>
    <version>1.0-SNAPSHOT</version>
    <modules>
        <module>health_common</module>
        <module>health_interface</module>
        <module>health_service_provider</module>
        <module>health_oms_backend</module>
        <module>health_oms_web</module>
        <module>health_job</module>
        <module>health_mobile_backend</module>
        <module>health_mobile_web</module>
        <module>health_sms_service_provider</module>
    </modules>
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <maven.compiler.source>1.7</maven.compiler.source>
        <maven.compiler.target>1.7</maven.compiler.target>
        <junit.version>4.13.1</junit.version>
        <servlet-api.version>2.5</servlet-api.version>
        <jsp-api.version>2.2</jsp-api.version>
        <!--C标签： c:if ，c:foreach-->
        <jstl.version>1.2</jstl.version>
        <spring.version>5.0.5.RELEASE</spring.version>
        <mybatis.version>3.4.5</mybatis.version>
        <mybatis-spring.version>1.3.1</mybatis-spring.version>
        <!--分页插件-->
        <pagehelper.version>4.1.4</pagehelper.version>
        <mysql-connector-java.version>5.1.38</mysql-connector-java.version>
        <druid.version>1.1.6</druid.version>
        <!--文件上传的-->
        <commons-fileupload.version>1.3.1</commons-fileupload.version>
        <!--解析json串的 ， jackson  -->
        <fastjson.version>1.2.47</fastjson.version>
        <tomcat7-maven-plugin.version>2.2</tomcat7-maven-plugin.version>
        <dubbo.version>2.6.2</dubbo.version>
        <zookeeper.version>3.4.13</zookeeper.version>
        <!--zookeeper 客户端-->
        <curator-framework.version>4.0.1</curator-framework.version>
        <!--操作redis的-->
        <jedis.version>2.9.0</jedis.version>
        <log4j.version>1.2.17</log4j.version>
        <slf4j.version>1.7.25</slf4j.version>
        <!-- 记录日志 -->
        <lombok.version>1.16.8</lombok.version>
        <qiniu.version>7.2.29</qiniu.version>
        <aliyun-java-sdk-core.version>4.5.0</aliyun-java-sdk-core.version>
        <httpclient.version>4.5.10</httpclient.version>
        <!-- 注意提取版本号 -->
        <spring.security.version>5.0.5.RELEASE</spring.security.version>
    </properties>
    <dependencyManagement>
